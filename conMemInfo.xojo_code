		Function SaveMemInfo() As Boolean
		  
		  Dim lsStr as String
		  Dim lnLen as Integer
		  Dim oSQL as new cSmartSQL
		  Dim rs as RecordSet
		  Dim lnRecNo as Integer
		  
		  
		  
		  'Me.MouseCursor = System.Cursors.Wait
		  
		  oSQL.StatementType = eStatementType.Type_Update
		  
		  oSql.ClearFields
		  oSQL.ClearValues
		  
		  oSQL.AddTable "tblpeople"
		  
		  
		  Dim ldBDay As Date
		  ldBDay = New Date
		  ldBDay.Month = txtBDayMonth.text.Val
		  ldBDay.Day = txtBDayDay.Text.Val
		  ldBDay.Year = txtBDayYear.Text.Val
		  
		  if cboChapterName.Text <> frmUpdateInfo.msPreviousChapter and frmUpdateInfo.mbIsMember then   '  Chapter has been update
		    NotifyOfChapterChange
		    mbChapterChanged = True
		    oSQL.AddFields  "ChapterName",            "Region",      "ChapterCode"
		    oSQL.AddValues  cboChapterName.Text,  msRegion, frmUpdateInfo.msNewChapterCode
		  end
		  
		  
		  oSQL.AddFields "NamePrefix",                "FirstName", "Middle",  "LastName",   "Birthday",                "Title"
		  oSQL.AddValues popNamePrefix.Text, txtFirst.Text, txtMiddle.Text, txtLast.Text, ldBDay.SQLDate, txtTitle.Text
		  
		  oSQL.AddFields  "PCompany",            "PAddress1",                 "PCity",               "PState",              "PZip",               "PCountry",             "PPhone"
		  oSQL.AddValues txtPCompany.Text,  txtPAddress1.Text, txtPCity.Text, txtPState.Text, txtPZip.Text, txtPCountry.Text, txtPPhone.Text
		  
		  
		  oSQL.AddFields  "SCompany",                  "SAddress1",             "SCity",          "SState ",          "SZip",          "SCountry",             "SPhone"
		  oSQL.AddValues txtSCompany.Text, txtSAddress1.Text, txtSCity.Text, txtSState.Text, txtSZip.Text, txtSCountry.Text, txtSPhone.Text
		  
		  
		  oSQL.AddFields              "Email",             "AlternateEmail" ,          "DateUpdated",     "UpdatedBy", "NameSuffix" ,   "CellPhone" ,          "BadgeName"
		  oSQL.AddValues txtPrimaryEmail.Text, txtSecondaryEmail.Text, Today.SQLDate,  "WebSite", txtNameSuffix.Text, txtPhoneCell.Text, txtBadge.Text
		  
		  
		  oSQL.AddFields  "ASHRAE",             "ASSE",             "PHCC",              "PCA",         "MCCA",          "IAPMO",                  "ICC",            "AYE"
		  oSQL.AddValues chkASHRAE.Value, chkASSE.Value, chkPHCC.Value, chkPCA.Value, chkMCCA.Value, chkIAPMO.Value, chkICC.Value, chkAYP.Value
		  
		  oSQL.AddFields "NSPE",        "NFPA",              "SFPE",              "WTO",              "ASME",             "MailInvoice"
		  oSQL.AddValues chkNSPE.Value, chkNFPA.Value, chkSFPE.Value, chkWTO.Value, chkASME.Value, chkMailInvoice.Value
		  
		  oSQL.AddSimpleWhereClause "PersonID",  Session.gnPersonID
		  
		  Session.sesAspeDB.SQLExecute(oSQL.SQL)
		  
		  
		  
		  if Session.sesAspeDB.CheckDBError then
		    MsgBox(Session.sesAspeDB.ErrorMessage)
		    Return False
		  end
		  
		  oSQL.StatementType = eStatementType.Type_Insert
		  
		  oSQL.ClearFromClause
		  oSQL.ClearValues
		  oSql.ClearFields
		  
		  oSQL.AddTable "tblNotes"
		  
		  oSQL.AddFields "PersonID",               "NoteDate",                    "Notes"
		  oSQL.AddValues Session.gnPersonID, ToDay.SQLDateTime, "Updated by: " +  txtFirst.Text + " " + txtLast.Text + " On Web Site"
		  
		  
		  Session.sesAspeDB.SQLExecute(oSQL.SQL)
		  
		  Dim lsSql as String
		  lsSql = "Select uid from d_users where mail='" +  txtPrimaryEmail.Text + "' and uid <> " + Str(Session.gnUid)
		  rs = Session.sesWebDB.SQLSelect(lsSql)
		  if Session.sesWebDB.Error then
		    MsgBox Session.sesWebDB.ErrorMessage
		    Return False
		  end
		  if rs.RecordCount > 0 then
		    lsSql = "Update d_users set mail = """ + "XXXX " + txtPrimaryEmail.Text + """ "+ "   where uid = " + Str(rs.Field("uid").StringValue)
		    Session.sesWebDB.SQLExecute(lsSql)
		    if Session.sesWebDB.Error then
		      MsgBox Session.sesWebDB.ErrorMessage
		      Return False
		    end
		  end
		  
		  
		  
		  if  msEmail <> txtPrimaryEmail.Text then
		    lsSql = "Update d_users set mail = """ + txtPrimaryEmail.Text + """  where uid = " + Str(Session.gnUid)
		    Session.sesWebDB.SQLExecute(lsSql)
		    if Session.sesWebDB.Error then
		      MsgBox Session.sesWebDB.ErrorMessage
		      Return False
		    end
		  end
		  
		  if Session.sesAspeDB.Error then
		    MsgBox Session.sesAspeDB.ErrorMessage
		    Return False
		  end
		  
		  
		  if mbChapterChanged then
		    frmUpdateInfo.SendNotification
		  end
		  
		  Return True
		  
		  
		End Function
