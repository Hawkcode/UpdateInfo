#tag Events cboChapterName
	#tag Event
		Sub LostFocus()
		  if me.ListIndex < 0 then
		    me.Style = EntryFieldsError
		  else
		    me.Style = EntryFields
		  end
		  
		End Sub
	#tag EndEvent
