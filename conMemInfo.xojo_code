		Sub NotifyOfChapterChange()
		  Dim rs as RecordSet
		  Dim lsSql as String
		  
		  Dim oSQL as new cSmartSQL
		  
		  oSQL.AutoBracket = False
		  
		  oSQL.AddTable "tblASPEChapterCodes"
		  oSQL.AddFields "RegionCode", "ChapterCode", "Satellite", "SatelliteOf"
		  oSQL.AddSimpleWhereClause "`ChapterName`", cboChapterName.Text
		  lsSQL = oSQL.SQL
		  
		  rs = Session.sesAspeDB.SQLSelect(lsSql)
		  
		  if Session.sesAspeDB.Error then return
		  
		  if rs <> nil then
		    frmUpdateInfo.msNewChapterCode =  rs.Field("ChapterCode").StringValue
		    msRegion = rs.Field("RegionCode").StringValue
		    'if rs.Field("Satellite").BooleanValue then
		    'lblSatOf.Text = "Satellite Of " + rs.Field("SatelliteOf").StringValue
		    'lblSatOf.Visible = True
		    'else
		    'lblSatOf.Visible = False
		    'end
		  end
		  
		  lsSql = "SELECT tblpeoplechapterofficers.PersonID, tblpeoplechapterofficers.ChapterCode, tblpeoplechapterofficers.OfficerCode, tblpeople.Email "
		  lsSql = lsSql + "FROM tblpeople INNER JOIN tblpeoplechapterofficers ON tblpeople.PersonID = tblpeoplechapterofficers.PersonID "
		  lsSql = lsSql + "WHERE (OfficerCode = 'C01P' or OfficerCode = 'C04VPM') and "
		  lsSql = lsSql + "(tblpeoplechapterofficers.ChapterCode = '" + frmUpdateInfo.msPreviousChapterCode + "' or "
		  lsSql = lsSql + "(tblpeoplechapterofficers.ChapterCode = '" + frmUpdateInfo.msNewChapterCode + "')) "
		  
		  rs = Session.sesAspeDB.SQLSelect(lsSql)
		  
		  if Session.sesAspeDB.Error then Return
		  
		  if rs.RecordCount > 0 then
		    While Not rs.EOF
		      if rs.Field("ChapterCode").StringValue = frmUpdateInfo.msPreviousChapterCode then
		        if rs.Field("OfficerCode").StringValue = "C01P" then
		          frmUpdateInfo.msPrevPresEmail = ChkStr(rs.Field("Email").StringValue)
		        else
		          frmUpdateInfo.msPrevVPMEmail = ChkStr(rs.Field("Email").StringValue)
		        end
		      else
		        if rs.Field("OfficerCode").StringValue = "C01P" then
		          frmUpdateInfo.msNewPresEmail = ChkStr(rs.Field("Email").StringValue)
		        else
		          frmUpdateInfo.msNewVPMEmail = ChkStr(rs.Field("Email").StringValue)
		        end
		      end
		      rs.MoveNext
		    Wend
		  end
		  
		  
		  
		  
		  
		  
		  
		  
		End Sub
	#tag EndMethod

